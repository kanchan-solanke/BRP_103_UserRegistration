<script>
const reg = /^([0-9]{2}) ([1-9]{1}[0-9]{9})$/

let str ="91 7386753242"

let result = reg.exec(str)
    console.log(result)

if(reg.test(str))
{
     console.log("Matches the expression: "+str)
}
else
{
    console.log("Please enter a valid Mobile number: "+str)
}
</script>
