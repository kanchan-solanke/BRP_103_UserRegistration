<script>
const reg = /^[A-Z]{1}[A-Za-z0-9]{7,}$/

let str ="KanchanSolanke"

let result = reg.exec(str)
    console.log(result)

if(reg.test(str))
{
     console.log("Matches the expression: "+str)
}
else
{
    console.log("Please enter a valid Password : "+str)
}
</script>
