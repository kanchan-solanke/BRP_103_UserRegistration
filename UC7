<script>
const reg = /^[A-Z]{1}[0-9]{1,}[A-Za-z0-9]{6,}$/

let str ="K6chanSolanke"

let result = reg.exec(str)
    console.log(result)

if(reg.test(str))
{
     console.log("Matches the expression: "+str)
}
else
{
    console.log("Please enter a valid Password at least one numeric value: "+str)
}
</script>
