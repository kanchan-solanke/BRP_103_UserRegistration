<script>
const reg = /^(.*[A-Z]{1,}.*[0-9]{1,}.*[~!@#$%^&*]?[A-Za-z0-9]{5,})$/

let str ="K6#chanSolanke"

let result = reg.exec(str)
    console.log(result)

if(reg.test(str))
{
     console.log("Matches the expression: "+str)
}
else
{
    console.log("Please enter a valid Password at least one special character: "+str)
}
</script>
